{
  "multipart": [
    {   "when": { "facing": "north" },
      "apply": { "model": "exsartagine:pot", "uvlock": true,"y": 0 }
    },
    {   "when": { "facing": "east" },
      "apply": { "model": "exsartagine:pot", "uvlock": true,"y": 90 }
    },
    {   "when": { "facing": "south" },
      "apply": { "model": "exsartagine:pot", "uvlock": true,"y": 180 }
    },
    {   "when": { "facing": "west" },
      "apply": { "model": "exsartagine:pot", "uvlock": true,"y": 270 }
    },
    {   "when": { "legs": "true" },
      "apply": { "model": "exsartagine:legs", "uvlock": true }
    },
    {   "when": { "full": "true" },
      "apply": { "model": "exsartagine:water", "uvlock": true }
    }
  ]
  }